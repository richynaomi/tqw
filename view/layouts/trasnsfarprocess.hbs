   function move() {
            var elem = document.getElementById("mybar");
            var width = 20;
            var id = setInterval(frame, 200);
            function frame() {
                if (width >= 41) {
                    clearInterval(id);
                } else {
                    width++;
                    elem.style.width = width + '%';
                    $("#mybar").html('<span class="ui-label" style="">Performing Transfer <b id="percentvalue" class="value">' + width + '%</b></span>');
                    if (width == 41){
                         $("#mybar").html('<span class="ui-label" style="">Continue Transaction! <b id="percentvalue" class="value">' + width + '%</b></span>');
                         $("#content").show();
                    }
                }
            }
}
$(document).ready(function(){
    $("#content").hide();
    move();
});
